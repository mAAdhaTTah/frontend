{
  "compilerOptions": {
    "target": "ES6",
    "checkJs": true,
    "lib": [
      "dom",
      "es2015",
      "es2016",
      "es2017",
      "es2018",
      "es2019",
      "es2020",
      "es2021",
      "es2022"
    ],
    "allowSyntheticDefaultImports": true,
    "module": "commonjs",
    "jsx": "react-jsx",
    "baseUrl": "./src",
    "paths": {
      "@app/*": ["@app/*"],
      "@strapi/*": ["@strapi/*"],
      "@tina/*": ["@tina/*"],
      "@ui/*": ["@ui/*"],
      "@wp/*": ["@wp/*"]
    }
  },
  "exclude": ["node_modules", "vercel"]
}
